[TimeLoop]
TEnd = 50.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s

[Grid]
LowerLeft = -5 -5
UpperRight = 5 5
Cells = 100 100
Overlap = 2

[Problem]
Name = erosion_2d_long2

[ModelParameters]
Omega = 8.0
PhiStar = 0.8
CorrelationLength = 0.15
FluidCompressibility = 1e-4
SolidDiffusivity = 0.0
FieldDiffusivity = 0.008
FieldRelaxationRate = 0.01
InitialStddev = 0.3

[BoundaryConditions]
RampUpTime = 10.0s
Evaporation = false
FlowRate = 0.4
InflowLength = 0.1
OutflowLength = 10.0
ObstacleType = None
JsonObstacleFile = "obstacles_2d.json"

[LinearSolver]
MaxIterations = 2000
Preconditioner.ILUOrder = 0
Preconditioner.Iterations = 1

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
