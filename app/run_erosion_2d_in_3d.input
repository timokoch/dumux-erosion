[TimeLoop]
TEnd = 50.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s
CheckPointPeriod = 0.25s

[Grid]
File = halfsphere.msh

[Problem]
Name = retina

[ModelParameters]
Omega = 8.0
PhiStar = 0.8
CorrelationLength = 0.05 # ζ
FluidCompressibility = 1e-4 # κ
FieldDiffusivity = 0.07 # D
FieldRelaxationRate = 100 # k_g
InitialStddev = 0.3

[BoundaryConditions]
RampUpTime = 10.0s
Evaporation = true
FlowRate = 0.4
PointSourcesStrength = 0.7 # retina
PointSourcePositions = -2.5 -4.33 0.0 # retina
ObstacleType = Json
JsonObstacleFile = "obstacles_retina.json"

[LinearSolver]
MaxIterations = 2000
Preconditioner.ILUOrder = 1
Preconditioner.Iterations = 2
Verbosity = 0

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
