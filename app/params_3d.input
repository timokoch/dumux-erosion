[TimeLoop]
TEnd = 50.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s

[Grid]
LowerLeft = -5 -5 -5
UpperRight = 5 5 5
Cells = 150 150 150
Overlap = 2

[Problem]
Name = erosion_3d

[ModelParameters]
Omega = 8.0
PhiStar = 0.8
CorrelationLength = 0.15
FluidCompressibility = 1e-4
SolidDiffusivity = 0.0
FieldDiffusivity = 0.008
FieldRelaxationRate = 0.01
InitialStddev = 0.2

[BoundaryConditions]
RampUpTime = 10.0s
Evaporation = false
FlowRate = 0.8
InflowLength = 0.2
OutflowLength = 5.0

[LinearSolver]
Preconditioner.AmgAccumulationMode = AtOnce
MaxIterations = 20

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
