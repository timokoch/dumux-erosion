[TimeLoop]
TEnd = 50.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s

[Grid]
LowerLeft = -2.5 -2.5 -1.25
UpperRight = 2.5 2.5 1.25
Cells = 100 100 50
Overlap = 4

[Problem]
Name = erosion_equal

[ModelParameters]
Omega = 6.5
PhiStar = 0.7
CorrelationLength = 0.2
FluidCompressibility = 1e-4
SolidDiffusivity = 0.0
FieldDiffusivity = 0.008
FieldRelaxationRate = 0.01

[BoundaryConditions]
RampUpTime = 10.0s
Evaporation = false
FlowRate = 0.8
InflowLength = 5.0
OutflowLength = 5.0

[LinearSolver]
Preconditioner.AmgAccumulationMode = AtOnce
MaxIterations = 20

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
