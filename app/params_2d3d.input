[TimeLoop]
TEnd = 100.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s

[Grid]
File = sphere.msh

[Problem]
Name = erosion_sphere

[ModelParameters]
Omega = 6.5
PhiStar = 0.7
CorrelationLength = 0.1
FluidCompressibility = 1e-4
SolidDiffusivity = 0.0
FieldDiffusivity = 0.004
FieldRelaxationRate = 0.01

[BoundaryConditions]
RampUpTime = 20.0s
Evaporation = true
FlowRate = 2.0

[LinearSolver]
Preconditioner.AmgAccumulationMode = AtOnce
MaxIterations = 20

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
