[TimeLoop]
TEnd = 50.0s
InitialTimeStepSize = 0.1s
MaxTimeStepSize = 10s

[Grid]
#File = halfsphere.msh
#File = sphere.msh
#File = hyperbolic.msh
File = ../../data/heart.msh

[Problem]
#Name = hyperbolic_symmetric
#Name = hyperbolic_evaporation
#Name = half_sphere_boundary
#Name = retina
#Name = sphere_symmetric
Name = heart3

[ModelParameters]
Omega = 8.0
PhiStar = 0.8
#CorrelationLength = 0.1
CorrelationLength = 0.8
FluidCompressibility = 1e-4
#FieldDiffusivity = 0.004
FieldDiffusivity = 0.4
FieldRelaxationRate = 0.01
InitialStddev = 0.3

[BoundaryConditions]
RampUpTime = 20.0s
Evaporation = false
FlowRate = 2.0
PointSourcesStrength = 5 5
PointSourcePositions = -14.6428 21.1399 37.5829 -23.9962 9.63808 23.1206

[LinearSolver]
Preconditioner.AmgAccumulationMode = AtOnce
MaxIterations = 20

[Newton]
EnablePartialReassembly = true

[Assembly]
BoxVolVarsDependOnAllElementDofs = true
